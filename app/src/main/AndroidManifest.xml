<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sagar.diabetesmanagement">

    <application
        android:allowBackup="true"

        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW" />

        <activity
            android:name=".home_activity"
            android:label="Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".all_activity_information"
            android:label="@string/EditActivity"/>
        <activity android:name=".list_of_activity_to_add"
            android:label="@string/AddActivity"/>
        <activity android:name=".history"
            android:label="@string/historyActivity"></activity>

    </application>

</manifest>